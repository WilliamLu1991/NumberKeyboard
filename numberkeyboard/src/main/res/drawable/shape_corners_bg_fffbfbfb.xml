<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置填充颜色 为灰色 -->
    <solid android:color="#FFFBFBFB" />

    <!-- 设置几何图形四个角的弧度 -->
    <corners android:radius="4dp"/>

    <!-- 设置几何形状绘制边框 红色边框 -->
    <stroke android:color="#CCCCCC"
        android:width="0.5dp"/>
</shape>